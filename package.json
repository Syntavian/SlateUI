{
  "name": "slate_ui",
  "version": "1.0.0",
  "description": "Slate UI",
  "scripts": {
    "prep": "npm install -ci && cd build/app && npm install -ci",
    "build": "python dev/slate_dev.py",
    "build-css": "npx sass dev/scss:dev/css",
    "prebuild-js": "npx swc ./dev/ts -d ./dev/js",
    "build-js": "npx spack",
    "test-py": "cd dev && python -m pytest python/",
    "test-js": "jest"
  },
  "author": "Syntavian",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  }
}
